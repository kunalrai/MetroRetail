      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0638A22B-928E-43E4-B16D-97BE025F9F71}"
          SQLTask:SqlStatementSource="MERGE INTO MetroRetailDW.dbo.DimPromotion AS TARGET&#xA;USING MetroRetail_stagingDW.dbo.DimPromotion AS SOURCE &#xA;ON (TARGET.PromotionKey = SOURCE.PromotionKey)&#xA;&#xA;--When records are matched, update the records if there is any change&#xA;WHEN MATCHED AND (&#xA;TARGET.PromotionLabel &lt;&gt; SOURCE.PromotionLabel OR&#xA;TARGET.PromotionName &lt;&gt; SOURCE.PromotionName OR&#xA;TARGET.PromotionDescription &lt;&gt; SOURCE.PromotionDescription OR&#xA;TARGET.DiscountPercent &lt;&gt; SOURCE.DiscountPercent OR&#xA;TARGET.PromotionType &lt;&gt; SOURCE.PromotionType OR&#xA;TARGET.PromotionCategory &lt;&gt; SOURCE.PromotionCategory OR&#xA;TARGET.StartDate &lt;&gt; SOURCE.StartDate OR&#xA;TARGET.EndDate &lt;&gt; SOURCE.EndDate OR&#xA;TARGET.MinQuantity &lt;&gt; SOURCE.MinQuantity OR&#xA;TARGET.MaQuantity &lt;&gt; SOURCE.MaQuantity OR&#xA;TARGET.ETLLoadID &lt;&gt; SOURCE.ETLLoadID OR&#xA;TARGET.LoadDate &lt;&gt; SOURCE.LoadDate OR&#xA;TARGET.UpdateDate &lt;&gt; SOURCE.UpdateDate) &#xA;THEN &#xA;UPDATE SET &#xA;TARGET.PromotionLabel = SOURCE.PromotionLabel,&#xA;TARGET.PromotionName = SOURCE.PromotionName,&#xA;TARGET.PromotionDescription = SOURCE.PromotionDescription,&#xA;TARGET.DiscountPercent = SOURCE.DiscountPercent,&#xA;TARGET.PromotionType = SOURCE.PromotionType,&#xA;TARGET.PromotionCategory = SOURCE.PromotionCategory,&#xA;TARGET.StartDate = SOURCE.StartDate,&#xA;TARGET.EndDate = SOURCE.EndDate,&#xA;TARGET.MinQuantity = SOURCE.MinQuantity,&#xA;TARGET.MaQuantity = SOURCE.MaQuantity,&#xA;TARGET.ETLLoadID = SOURCE.ETLLoadID,&#xA;TARGET.LoadDate = SOURCE.LoadDate,&#xA;TARGET.UpdateDate = SOURCE.UpdateDate&#xA;&#xA;--When no records are matched, insert the incoming records from source table to target table&#xA;WHEN NOT MATCHED BY TARGET THEN &#xA;INSERT (&#xA;PromotionKey,&#xA;PromotionLabel,&#xA;PromotionName,&#xA;PromotionDescription,&#xA;DiscountPercent,&#xA;PromotionType,&#xA;PromotionCategory,&#xA;StartDate,&#xA;EndDate,&#xA;MinQuantity,&#xA;MaQuantity,&#xA;ETLLoadID,&#xA;LoadDate,&#xA;UpdateDate)&#xA;VALUES (&#xA;SOURCE.PromotionKey,&#xA;SOURCE.PromotionLabel,&#xA;SOURCE.PromotionName,&#xA;SOURCE.PromotionDescription,&#xA;SOURCE.DiscountPercent,&#xA;SOURCE.PromotionType,&#xA;SOURCE.PromotionCategory,&#xA;SOURCE.StartDate,&#xA;SOURCE.EndDate,&#xA;SOURCE.MinQuantity,&#xA;SOURCE.MaQuantity,&#xA;SOURCE.ETLLoadID,&#xA;SOURCE.LoadDate,&#xA;SOURCE.UpdateDate);&#xA;&#xA;--OPTIONAL: If you want to delete records in target that are not in source&#xA;--WHEN NOT MATCHED BY SOURCE THEN &#xA;--DELETE;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
